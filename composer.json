{
	"name": "sergey-bel/php-classfuzz",
	"description": "Fuzzer for php classes",
	"authors": [
		{
			"name": "Sergey Belov"
		}
	],

	"autoload": {
        "psr-4": {
		"PhpClassFuzz\\": "./src",
		"PhpClassFuzz\\Tests\\": "./tests"
		}
    },
	
    "require": {
		"symfony/dependency-injection": "^6.0",
		"symfony/console": "^6.0",
		"symfony/yaml": "^6.0",
		"symfony/config": "^6.0",
		"symfony/finder": "^6.0",
		"phpunit/php-code-coverage": "^9.2"
	},
	"require-dev": {
		"symfony/var-dumper": "^6.0",
		"friendsofphp/php-cs-fixer": "^3.6",
		"phpunit/phpunit": "^9.5",
		"phpstan/phpstan": "^1.5"
	},
	"bin": ["bin/php-classfuzz"]
}
